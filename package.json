    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "figlet": "^1.8.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "ora": "^8.1.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
