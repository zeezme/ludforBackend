{
  "name": "ludfor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "scripts": {
    "create:module": "tsx scripts/createModule.tsx",
    "remove:module": "cross-env REMOVE_MODULE=true tsx scripts/createModule.tsx",
    "db:migrate": "tsx scripts/migration-runner.tsx",
    "build": "tsc --experimentalDecorators",
    "start": "node dist/app.js",
    "start:dev": "set NODE_OPTIONS=--inspect=0.0.0.0:9229 && nodemon --watch src --ext ts --exec \"tsx watch src/app.ts\"",
